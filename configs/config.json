{
    "appPort": "{{APP_PORT}}",
    "redis": {
        "host" : "{{REDIS_HOST}}",
        "port" : "{{REDIS_PORT}}"
    },
    "db" : {
        "username": "{{DB_USERNAME}}",
        "password": "{{DB_PASSWORD}}",
        "database": "{{DB_DATABASE}}",
        "dialect": "{{DB_DIALECT}}",
        "host": "{{DB_HOST}}",
        "port": "{{DB_PORT}}"
    },
    "test-db": {
        "username": "{{TEST_DB_USERNAME}}",
        "password": "{{TEST_DB_PASSWORD}}",
        "database": "{{TEST_DB_DATABASE}}",
        "dialect": "{{TEST_DB_DIALECT}}",
        "host": "{{TEST_DB_HOST}}",
        "port": "{{TEST_DB_PORT}}"
    },
    "mail": {
        "from": "{{MAIL_FROM}}",
        "transport": "{{MAIL_TRANSPORT}}",
        "transportOptions": {
            "host": "{{MAIL_HOST}}",
            "port": "{{MAIL_PORT}}",
            "auth": {
                "user": "{{MAIL_AUTH_USER}}",
                "pass": "{{MAIL_AUTH_PASS}}"
            }
        }
    },
    "sessions" : {
        "secure" : {{SESSIONS_SECURE}},
        "secret":  "{{SECRET}}",
        "name" : "cloud.sid",
        "expires": 43200000
    }
}
